{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "https://blog.golang.jp/",
        "type": 302
      },
      {
        "source": "/:path*",
        "destination": "https://blog.golang.jp/:path",
        "type": 302
      }
    ]
  }
}
